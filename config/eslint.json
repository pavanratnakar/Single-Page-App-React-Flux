{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": [
        "react"
    ],
    "globals": {
        "$": false,
        "jQuery": false,
        "Handlebars": false
    },
    "rules": {
        "no-underscore-dangle": false,
        "semi": 2
    },
    "ecmaFeatures": {
        "blockBindings": true,
        "forOf": true,
        "jsx": true
    }
}